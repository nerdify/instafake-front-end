{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "react-app-rewired build",
    "download-schema": "node scripts/downloadSchema.js",
    "fix": "eslint --fix \"**/*.{js,ts,tsx}\"",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\" \"{.babelrc,.eslintrc}\"",
    "generate": "graphql-codegen",
    "postinstall": "node scripts/fixNoFallthroughCasesInSwitchError.js",
    "prestart": "yarn relay && yarn generate",
    "relay": "yarn download-schema && relay-compiler",
    "start": "rimraf node_modules/.cache && BROWSER=none FAST_REFRESH=true react-app-rewired start",
    "test": "react-app-rewired test"
  },
  "dependencies": {
    "@chakra-ui/react": "1.0.0",
    "@emotion/react": "11.0.0",
    "@emotion/styled": "11.0.0",
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/pro-light-svg-icons": "5.15.1",
    "@fortawesome/pro-regular-svg-icons": "5.15.1",
    "@fortawesome/pro-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.11",
    "date-fns": "2.16.1",
    "dot-prop": "6.0.0",
    "extract-files": "9.0.0",
    "formik": "2.2.0",
    "framer-motion": "2.9.4",
    "history": "5.0.0",
    "lodash.set": "4.3.2",
    "pusher-js": "7.0.0",
    "react": "0.0.0-experimental-4ead6b530",
    "react-dom": "0.0.0-experimental-4ead6b530",
    "react-error-boundary": "3.0.2",
    "react-relay": "0.0.0-experimental-183bdd28",
    "react-relay-mutation": "0.2.3",
    "react-router-dom": "0.0.0-experimental-ffd8c7d0",
    "react-scripts": "4.0.0-next.77",
    "react-textarea-autosize": "8.2.0",
    "react-use": "15.3.4",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@graphql-codegen/add": "2.0.1",
    "@graphql-codegen/cli": "1.17.10",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.7",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "@types/react-relay": "7.0.13",
    "@types/relay-runtime": "10.0.4",
    "@types/yup": "0.29.8",
    "babel-plugin-relay": "10.0.1",
    "customize-cra": "1.0.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-relay": "1.8.1",
    "graphql": "15.3.0",
    "prettier": "2.1.2",
    "react-app-rewired": "2.1.6",
    "relay-compiler": "10.0.1",
    "relay-compiler-language-typescript": "13.0.1",
    "relay-config": "10.0.1",
    "rimraf": "3.0.2",
    "stop-runaway-react-effects": "2.0.0",
    "typescript": "4.0.3"
  }
}
